# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# TAAPI.IO Configuration
TAAPI_API_KEY=your_taapi_api_key

# CoinGecko Configuration
COINGECKO_API_KEY=your_coingecko_api_key

# API Configuration
# Default cryptocurrency (BTC, ETH, ADA, SOL, DOT, LINK, MATIC, LTC, XRP, DOGE)
DEFAULT_CRYPTO=BTC
DEFAULT_EXCHANGE=binance
DEFAULT_INTERVAL=1h

# Supported Cryptocurrencies (comma-separated list)
# Used for batch operations, cron jobs, and monitoring
# Recommended: BTC,ETH,SOL (uses ~864 requests/day = 15% of free tier)
SUPPORTED_CRYPTOS=BTC,ETH,SOL

# Environment
ENVIRONMENT=development
